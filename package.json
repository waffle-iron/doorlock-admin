{
  "name": "doorlock-admin",
  "version": "1.0.0",
  "description": "Doorlock admin UI",
  "scripts": {
    "clean": "rimraf static/js static/css/main.css static/css/main.css.map",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "staging": "cross-env NODE_ENV=production && node prodServer.js",
    "lint": "eslint src",
    "storybook": "start-storybook -p 9001 -s ./static",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.26.0",
    "alt": "^0.18.2",
    "alt-container": "^1.0.2",
    "axios": "^0.11.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.8.5",
