    "react-dom": "^15.0.2",
    "react-notification-system": "^0.2.7",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-bootstrap": "^0.23.0",
    "redux": "^3.6.0",
    "rimraf": "^2.4.3",
    "socket.io-client": "^1.4.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
